<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="csdldemo2">
        <class>Entities.Phone</class>
        <class>Entities.Employee</class>
        <class>Entities.Class</class>
        <class>Entities.Person</class>
        <class>Entities.Parent</class>
        <class>Entities.Child</class>
        <class>Entities.One</class>
        <class>Entities.Two</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/csdldemo2"/>
            <property name="javax.persistence.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.user" value="root" />
            <property name="javax.persistence.jdbc.password" value="" />
            <property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect"/>
            <property name="hibernate.show_sql" value="true" />
        </properties>
    </persistence-unit>
</persistence>
